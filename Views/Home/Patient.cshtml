            }


            $('.account-settings--change-password #resetPassword').click(function () {
                var ChangePasswordModel = {
                    OldPassword: $('#oldPassword').val(),
                    NewPassword: $('#newPassword').val(),
                    ConfirmPassword: $('#repeatPassword').val()
                };
                $.ajax({
                    url: '/Account/ChangePassword/',
                    type: 'Post',
                    data: JSON.stringify(ChangePasswordModel),
                    contentType: "application/json;charset=utf-8",
                    success: function (result) {
                        $('.success, .warning, .attention, .information, .error').remove();
                        if (result.Success != null) {
                            $('#notification').html('<div class="success" style="display: none;">' + result.Success + '<img src="../../Images/close.png"  alt="" class="closen" /></div>');
                            $('.success').fadeIn('slow');
                        }
                        else if (result.Error != null) {
                            $('#notification').html('<div class="error" style="display: none;">' + result.Error + '<img src="../../Images/close.png" alt="" class="closen" /></div>');
                            $('.error').fadeIn('slow');
                        }

                    },
                    error: function () {
                    }
                });
            });

            $('.success img, .error img').live('click', function () {
                $(this).parent().fadeOut('slow', function () {
                    $(this).remove();
                });
            });


            $('.account-settings').click(function () {
                $.ajax({
                    url: '/Account/PersonalInfo/',
                    type: 'Post',
                    contentType: "application/json;charset=utf-8",
                    success: function (updateinfo) {
                        $('#EPatientName').val(updateinfo.PatientName);
                        $('#Sex').val(updateinfo.Sex);
                        $('#Edob').val(updateinfo.DOB);
                        $('#EEmailId').val(updateinfo.EmailId);
                    },
                    error: function () {
                    }
                });
            });

            $('#Edit').click(function () {
                $('.form--fieldset--personal-info').find('.edit').removeAttr('disabled');

            });

            $('#Update').click(function () {
                var UpdateInfo = {
                    PatientName: $('#EPatientName').val(),
                    DOB: $('#Edob').val(),
