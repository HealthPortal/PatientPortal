                        }
                        else if (result.Error != null) {
                            $('#notify').html('<div class="error" style="display: none;">' + result.Error + '<img src="../../Images/close.png" alt="" class="closen" /></div>');
                            $('.error').fadeIn('slow');
                        }
                    },
                    error: function () {
                    }
                });
            });

        });




