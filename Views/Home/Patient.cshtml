        function myhealthactivity() {
            $('.stream-entries .acc-hide').hide();
            $('.stream-entry--account-settings').hide();
            $('.stream-entry--insurance-billing').hide();
            $(".container").find(".navbar").removeClass("shown");
            $(".row").find(".sidebar").removeClass("shown");
            $(".row").find(".stream").removeClass("shown");
            $(".control-panel").find(".affix").removeClass("shown");
            $(".controler").find(".nav-tab-wrap").removeClass("shown");
            $(".row").find(".stream").css({ "opacity": "1", "z-index": "3" });
            $(".container .span7").toggleClass('madjust');
            var winWidth = $(window).width();
            if (winWidth <= 768) {
                $(".control-panel").find(".affix").css({ "opacity": "0", "z-index": "-3" });
            }
            else {
                $(".control-panel").find(".affix").css({ "opacity": "0", "z-index": "3" });
            }

            $.ajax({
                url: '@Url.Action("HealthActs", "Healthactivity")',
                type: 'Post',
                contentType: "application/json;charset=utf-8",
                success: function (healthmodels) {
                    if (healthmodels != null) {
                        $.each(healthmodels, function (key, value) {
                            if (value.LabResult != null) {
                                var templcontent = $('#healthLabResults').find('.entry-wrapper .entry-main');
                                $(templcontent).find('.entry-header').find('.DatePerformed').html(value.LabResult.DatePerformed);
                                $(templcontent).find('.entry-body .info-section--table').find('.info-name').html(value.LabResult.OrderableTestName);
                                $(templcontent).find('.entry-body .info-section--table').find('.meta-labLOINCCode').html(value.LabResult.LONICCode);
                                $(templcontent).find('.entry-body .info-section--table').find('.meta-TestResultValue').html(value.LabResult.TestResultValue);

                                $('.stream-entry--labresults').addClass('health-stream');
                                var Labtemp = $('#healthLabResults').html();
                                $('.stream .stream-entries').append(Labtemp);

                            }
                            else if (value.AppointmentNew != null) {

                                var tempacontent = $('#healthApptmnt');
                                $(tempacontent).find('.entry-wrapper').find('.LabDate').html(value.AppointmentNew.Date);
                                $(tempacontent).find('.entry-main').find('.appt-time').html(value.AppointmentNew.Time);
                                $(tempacontent).find('.entry-main').find('.reason').html(value.AppointmentNew.Reason);
                                $(tempacontent).find('.entry-main').find('.Physician').html(value.AppointmentNew.Physician);
                                $(tempacontent).find('.user-contact .user-contact-info').find('.CenterName').html(value.AppointmentNew.CenterName);
                                $(tempacontent).find('.user-contact .user-contact-info').find('.info-Address').html(value.AppointmentNew.Address);

                                $('.stream-entry--appointment').addClass('health-stream');
                                var Aptmnttemp = $('#healthApptmnt').html();
                                $('.stream .stream-entries').append(Aptmnttemp);
                            }
                            else if (value.Medication != null) {
                                var tempcontent = $('#healthmedications').find('.entry-wrapper .entry-main');
                                $(tempcontent).find('.entry-header').find('.date-prescription').html(value.Medication.DateOfPrescription);
                                $(tempcontent).find('.entry-body .medication-info').find('.info-name').html(value.Medication.MedicationsName);
                                $(tempcontent).find('.entry-body .medication-info').find('.meds-StandardCode').html(value.Medication.StandardCode);
                                $(tempcontent).find('.entry-body .medication-info').find('.medication--status').html(value.Medication.Status);
                                $(tempcontent).find('.entry-body .medication-info').find('.medication--sig').html(value.Medication.SIG);
                                $('.stream-entry--medication').addClass('health-stream');
                                var medtemp = $('#healthmedications').html();
                                $('.stream .stream-entries').append(medtemp);
                            }

                        });

                    }
                },
                error: function () {
                }

            });

        }

